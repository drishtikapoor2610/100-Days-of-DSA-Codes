#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    int i, j;
    char temp;

    // Input
    scanf("%s", str);

    // Initialize pointers
    i = 0;
    j = strlen(str) - 1;

    // Reverse the string
    while (i < j) {
        temp = str[i];
        str[i] = str[j];
        str[j] = temp;

        i++;
        j--;
    }

    // Output
    printf("%s", str);

    return 0;
}
